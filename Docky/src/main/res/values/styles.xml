<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:actionBarStyle">@style/CustActionBar</item>
    </style>

    <!-- ActionBar styles -->
    <style name="CustActionBar" parent="android:Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/ab_background</item>
    </style>

    <!-- Dock styles -->
    <style name="Dock">
    </style>

    <style name="Dock.Black">
        <item name="android:background">@drawable/dock_background_black_rounded</item>
    </style>

    <style name="Dock.White">
        <item name="android:background">@drawable/dock_background_white_rounded</item>
    </style>

</resources>
